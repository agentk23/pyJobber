{
    "manifest_version": 3,
    "name": "GDPR Cookie Popup Blocker",
    "version": "1.0",
    "description": "Automatically handles GDPR cookie popups",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_start"
        }
    ],
    "action": {
        "default_popup": "popup.html"
    }
}